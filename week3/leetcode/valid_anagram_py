class Solution(object):
    def isAnagram(self, s, t):
        """
        :type s: str
        :type t: str
        :rtype: bool
        """
        s_arr = sorted(list(s))
        t_arr = sorted(list(t))
        if s == t:
            return True
        
        if len(s) != len(t):
            return False
        for i in range(len(s)):
            if s_arr[i] != t_arr[i]:
                return False
        
        return True